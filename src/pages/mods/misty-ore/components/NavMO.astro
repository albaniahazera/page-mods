---
const navUrl_1 = "/mods/misty-ore"
const navUrl_2 = "/mods/misty-ore/gallery"
---


<div class="container">
	<div class="container-hom-gal">
		<h4 class="hom-gal">
			<a href={navUrl_1} class="link">
				Main
			</a>
		</h4>
	</div>
	<div class="container-hom-gal">
		<h4 class="hom-gal">
			<a href={navUrl_2} class="link">
				Gallery
			</a>
		</h4>
	</div>
</div>


<style>
	.container {
        align-items: center;
        display: flex;
        position: absolute;
        right: 0;
        margin: 0px 4px;
    }
    
    .container-hom-gal {
        margin-left: 4px;
		margin-right: 4px;
    }
	
    
    .hom-gal {
        padding: 2px;
		justify-content: center;
		cursor: pointer;
		display: flex;
		align-items: center;
		border: 1px solid rgb(255, 255, 255);
    }
    
	.hom-gal:hover {
		background-color: rgb(0, 0, 0);
	}

    .link {
		text-decoration: none;
		color: rgb(255, 255, 255);
    }
	
	
</style>